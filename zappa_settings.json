{
    "dev": {
        "app_function": "comments.app",
        "aws_region": "eu-west-1",
        "profile_name": "default",
        "project_name": "comments",
        "runtime": "python3.6",
        "s3_bucket": "zappa-comments-build",
        "events": [{
            "function": "comments.rebuild_comment_index",
            "event_source": {
                "arn":  "arn:aws:s3:::zappa-comments-input",
                "events": [
                    "s3:ObjectCreated:*",
                    "s3:ObjectRemoved:*"
                ]
           }
        }]
    }
}
